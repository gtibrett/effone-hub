{
	"name": "@gtibrett/effone-hub-graph-api",
	"version": "2.0.0-alpha.0",
	"license": "MIT",
	"main": "src/index.ts",
	"dependencies": {
		"@graphile-contrib/pg-simplify-inflector": "^6.1.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"csv": "^6.3.1",
		"decompress": "^4.2.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"helmet": "^7.0.0",
		"morgan": "^1.10.0",
		"postgraphile": "^4.13.0",
		"sequelize": "^6.28.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.5",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@types/compression": "^1.7.3",
		"@types/cors": "^2.8.13",
		"@types/decompress": "^4.2.4",
		"@types/express": "^4.17.15",
		"@types/morgan": "^1.9.5",
		"@types/node": "^18.17.1",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"eslint": "^8.57.0",
		"graphql": "^16",
		"ts-node": "^10.9.1",
		"typescript": "^5.4.4"
	},
	"scripts": {
		"lint": "eslint src",
		"clean": "rm -fr ./dist tsconfig.tsbuildinfo",
		"start": "npx ts-node src/index.ts",
		"prebuild": "yarn clean && yarn compile",
		"compile": "graphql-codegen",
		"build": "tsc",
		"postbuild": "yarn copypublic && yarn copydeps && yarn copyschema",
		"copypublic": "cp -r public dist/",
		"precopydeps": "cp -r package.json README.md public dist/",
		"copydeps": "cd dist && yarn install --production=true",
		"postcopydeps": "rm dist/yarn.lock",
		"copyschema": "cp src/schema.graphql dist/",
		"prepublish-package": "yarn build",
		"publish-package": "sh ./scripts/pre-publish.sh",
		"package": "rm effone-hub-graph-api.tgz && cd dist && tar -zcf ../effone-hub-graph-api.tgz *"
	}
}
